<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dialog_edit_number_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/background_gadient">

<ScrollView
	android:layout_width="match_parent"
	android:layout_height="300dp"
	android:id="@+id/dialog_statistics_sqc_table_container">





</ScrollView>




	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_below="@+id/dialog_statistics_sqc_table_container"
		>


		<Button
			android:id="@+id/dialog_communication_config_button_save"
			android:layout_width="match_parent"
			android:layout_height="50dp"
			android:layout_weight="1"
			android:text="@string/save"
			style="@style/button_default"
			/>


		<Button
			android:id="@+id/dialog_communication_config_button_close"
			android:layout_width="match_parent"
			android:layout_height="50dp"
			android:layout_weight="1"
			android:text="@string/close"
			style="@style/button_default"

			/>


	</LinearLayout>



</RelativeLayout>