<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_actionbar"
    android:layout_width="match_parent"
    android:soundEffectsEnabled="true"
    android:layout_height="60dp" >


 <LinearLayout 
     android:layout_height="match_parent"
     android:layout_width="match_parent"
     android:layout_toRightOf="@+id/naviagationbar_button_home"
     android:layout_toLeftOf="@+id/naviagationbar_button_settings"
     android:orientation="horizontal">

     <Button
         android:id="@+id/actionbar_button_zero"
         android:text="@string/zero"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />
     
   <Button 
     android:id="@+id/actionbar_button_tara"
     android:text="@string/tare"
     android:textAllCaps="true"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:layout_weight="1"
       style="@style/button_actionbar"
     />
    <Button 
     android:id="@+id/actionbar_button_protocol"
     android:text="@string/protocol"
     android:textAllCaps="true"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:layout_weight="1"
        style="@style/button_actionbar"
     />
   <Button 
     android:id="@+id/actionbar_button_cal"
     android:text="@string/cal"
     android:textAllCaps="true"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:layout_weight="1"
       style="@style/button_actionbar"
     />
     <Button
         android:id="@+id/actionbar_button_statistics"
         android:text="@string/stats_0"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />
     <Button
         android:id="@+id/actionbar_button_new_batch"
         android:text="@string/new_batch"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />
     <Button
         android:id="@+id/actionbar_button_end_batch"
         android:text="@string/end_batch"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />
     <Button
         android:id="@+id/actionbar_button_view_batch"
         android:text="@string/show"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />



     <Button
         android:id="@+id/actionbar_button_accumulate"
         android:text="@string/add_to_stats"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />

     <Button
         android:id="@+id/actionbar_button_start"
         android:text="@string/start"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />





     <Button
         android:id="@+id/actionbar_button_Ingredient_List"
         android:text="@string/cost_list"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />

     <Button
         android:id="@+id/actionbar_button_accept"
         android:text="@string/accept"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />

     <Button
         android:id="@+id/actionbar_button_end"
         android:text="@string/end"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />



     <Button
         android:id="@+id/actionbar_button_settings"
         android:text="@string/settings"
         android:textAllCaps="true"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_weight="1"
         style="@style/button_actionbar"
         />

 </LinearLayout>



</RelativeLayout>
